<!DOCTYPE html>
<html>
  <head>
    <title><%= yield(:title) %> | <%= t ".Ruby" %> </title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag "application", media: "all",
      "data-turbolinks-track": "reload" %>
    <%= javascript_tag "application", "data-turbolinks-track": "reload" %>
  </head>

  <body>
    <%= link_to t(".english"), locale: "en" %>
    <%= link_to t(".vietnamese"), locale: "vi" %>
    <%= yield %>
  </body>
</html>
